@model WebApplication1.Models.GoogleRequestModel


<div>
    @foreach (var item in Model.itemListElement)
    {
        <div id="suggestionToSelect" style="border: 1px solid cornflowerblue; padding: 0 1vw">
            <input type="hidden" name="commonName" value="@ViewBag.commonName" />
            <h1 hidden>@item.result.id</h1>
            @if (item.result != null) {
                <h4>@item.result.name</h4>
                <h5>@item.result.description</h5>
                if (item.result.image != null) {
                    <img style="max-width:100%; max-height:30vh" src="@item.result.image.contentUrl" alt="@item.result.name" />
                }
                if (item.result.detailedDescription != null) {
                    <p>@item.result.detailedDescription.articleBody</p>
                }
            }
            <div><button class="btn btn-primary active" type="button" id="selectSuggestion">Select this Suggestion</button></div>
        </div>
    }
</div>
